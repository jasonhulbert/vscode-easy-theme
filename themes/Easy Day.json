	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#B0B0B5"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
                "punctuation"
			],
			"settings": {
				"foreground": "#3E3E40"
			}
		},
		{
			"name": "Braces and Brackets",
			"scope": [
				"meta.brace",
				"meta.bracket"
			],
			"settings": {
				"foreground": "#3E3E40"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"punctuation.definition.string.begin",
				"punctuation.definition.string.end"
			],
			"settings": {
				"foreground": "#DB8C53"
			}
		},
		{
			"name": "Regex",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#D16347"
			}
		},
		{
			"name": "Template Expression, String Interpolation",
			"scope": [
				"punctuation.definition.template-expression",
				"punctuation.definition.interpolation"
			],
			"settings": {
