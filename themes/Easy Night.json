	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#4C437D"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
                "punctuation"
			],
			"settings": {
				"foreground": "#AEB1B8"
			}
		},
		{
			"name": "Braces and Brackets",
			"scope": [
				"meta.brace",
				"meta.bracket"
			],
			"settings": {
				"foreground": "#AEB1B8"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"punctuation.definition.string.begin",
				"punctuation.definition.string.end"
			],
			"settings": {
				"foreground": "#F6AB77"
			}
		},
		{
			"name": "Regex",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#EB8167"
			}
		},
		{
			"name": "Template Expression, String Interpolation",
