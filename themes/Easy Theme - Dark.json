	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#6E6B79"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"punctuation.definition.string.begin",
				"punctuation.definition.string.end"
			],
			"settings": {
				"foreground": "#CBD1A9"
			}
		},
		{
			"name": "Regex",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#B39988"
