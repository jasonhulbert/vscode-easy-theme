	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#BBBBBF"
			}
		},
		{
			"name": "Braces and Brackets",
			"scope": [
				"meta.brace",
				"meta.bracket"
			],
			"settings": {
				"foreground": "#525254"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"punctuation.definition.string.begin",
				"punctuation.definition.string.end"
			],
			"settings": {
				"foreground": "#F69078"
			}
		},
		{
			"name": "Regex",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#EB7C82"
			}
		},
		{
			"name": "Template Expression, String Interpolation",
