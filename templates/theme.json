		"list.inactiveFocusBackground": "<%= accentAlpha20 %>",
		"list.invalidItemForeground": "<%= red %>",
		"list.errorForeground": "<%= red %>",
		"list.warningForeground": "<%= orange %>",
		"listFilterWidget.background": "<%= background %>",
		"listFilterWidget.outline": "<%= border %>",
		"listFilterWidget.noMatchesOutline": "<%= border %>",
		"list.filterMatchBackground": "<%= accent %>",
