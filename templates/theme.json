		"// Activity Bar": "//",
		"activityBar.background": "<%= background %>",
		"activityBar.dropBackground": "<%= accentAlpha60 %>",
		"activityBar.foreground": "<%= foreground %>",
		"activityBar.inactiveForeground": "<%= comment %>",
		"activityBar.border": "<%= border %>",
		"activityBarBadge.background": "<%= accent %>",
		"activityBarBadge.foreground": "<%= brightBackground %>",
		"activityBar.activeBorder": "<%= accent %>",
		"activityBar.activeBackground": "<%= accentAlpha20 %>",
		
		"// Sidebar": "//",
		"sideBar.background": "<%= background %>",
		"sideBar.foreground": "<%= foreground %>",
		"sideBar.border": "<%= border %>",
		"sideBar.dropBackground": "<%= accentAlpha60 %>",
		"sideBarTitle.foreground": "<%= comment %>",
		"sideBarSectionHeader.background": "<%= background %>",
		"sideBarSectionHeader.foreground": "<%= foreground %>",
		
		"// Minimap": "//",
		"minimap.findMatchHighlight": "<%= brightForeground %>",
		"minimap.selectionHighlight": "<%= accent %>",
		
		"// Editor Group": "//",
		"editorGroup.border": "<%= border %>",
		"editorGroup.dropBackground": "<%= accentAlpha60 %>",
		"editorGroupHeader.noTabsBackground": "<%= background %>",
		"editorGroupHeader.tabsBackground": "<%= background %>",
		"editorGroupHeader.tabsBorder": "<%= transparent %>",
		"editorGroup.emptyBackground": "<%= background %>",
		"editorGroup.focusedEmptyBorder": "<%= background %>",
		"tab.activeBackground": "<%= altBackground %>",
		"tab.activeForeground": "<%= altForeground %>",
		"tab.unfocusedActiveBackground": "<%= altBackground %>",
		"tab.border": "<%= transparent %>",
		"tab.activeBorder": "<%= altBackground %>",
		"tab.unfocusedActiveBorder": "<%= transparent %>",
		"tab.activeBorderTop": "<%= altBackground %>",
		"tab.unfocusedActiveBorderTop": "<%= background %>",
		"tab.inactiveBackground": "<%= background %>",
		"tab.inactiveForeground": "<%= comment %>",
		"tab.unfocusedActiveForeground": "<%= comment %>",
		"tab.unfocusedInactiveForeground": "<%= comment %>",
		"tab.hoverBackground": "<%= altBackground %>",
		"tab.unfocusedHoverBackground": "<%= altBackground %>",
		"tab.hoverBorder": "<%= transparent %>",
		"tab.unfocusedHoverBorder": "<%= background %>",
		"tab.activeModifiedBorder": "<%= modified %>",
		"tab.inactiveModifiedBorder": "<%= modifiedBackground %>",
		"tab.unfocusedActiveModifiedBorder": "<%= modifiedBackground %>",
		"tab.unfocusedInactiveModifiedBorder": "<%= modifiedBackground %>",
		"editorPane.background": "<%= background %>",
		
		"// Editor": "//",
		"editor.background": "<%= altBackground %>",
		"editor.foreground": "<%= altForeground %>",
		"editorLineNumber.foreground": "<%= border %>",
		"editorLineNumber.activeForeground": "<%= accent %>",
		"editorCursor.background": "<%= accent %>",
		"editorCursor.foreground": "<%= accent %>",
		"editor.selectionBackground": "<%= selectionAlpha60 %>",
		"editor.selectionForeground": "<%= brightForeground %>",
		"editor.inactiveSelectionBackground": "<%= selectionAlpha20 %>",
		"editor.selectionHighlightBackground": "<%= selectionAlpha20 %>",
		"editor.selectionHighlightBorder": "<%= selectionAlpha80 %>",
		"editor.wordHighlightBackground": "<%= selectionAlpha20 %>",
		"editor.wordHighlightBorder": "<%= transparent %>",
		"editor.wordHighlightStrongBackground": "<%= selectionAlpha40 %>",
		"editor.wordHighlightStrongBorder": "<%= selection %>",
		"editor.findMatchBackground": "<%= selectionAlpha40 %>",
		"editor.findMatchHighlightBackground": "<%= selectionAlpha40 %>",
		"editor.findRangeHighlightBackground": "<%= selectionAlpha20 %>",
		"editor.findMatchBorder": "<%= selection %>",
		"editor.findMatchHighlightBorder": "<%= transparent %>",
		"editor.findRangeHighlightBorder": "<%= transparent %>",
		"editor.hoverHighlightBackground": "<%= selectionAlpha20 %>",
		"editor.lineHighlightBackground": "<%= background %>",
		"editor.lineHighlightBorder": "<%= background %>",
		"editor.rangeHighlightBackground": "<%= selectionAlpha20 %>",
		"editor.rangeHighlightBorder": "<%= transparent %>",
		"editorLink.activeForeground": "<%= accent %>",
		"editorWhitespace.foreground": "<%= border %>",
		"editorIndentGuide.background": "<%= border %>",
		"editorIndentGuide.activeBackground": "<%= accentAlpha80 %>",
		"editorRuler.foreground": "<%= border %>",
		"editorCodeLens.foreground": "<%= submodule %>",
		"editorLightBulb.foreground": "<%= accent %>",
		"editorLightBulbAutoFix.foreground": "<%= green %>",
		"editorBracketMatch.border": "<%= accent %>",
		"editorError.foreground": "<%= red %>",
		"editorWarning.foreground": "<%= orange %>",
		"editorInfo.foreground": "<%= blue %>",
		"editorHint.foreground": "<%= cyan %>",
		"problemsErrorIcon.foreground": "<%= red %>",
		"problemsWarningIcon.foreground": "<%= orange %>",
		"problemsInfoIcon.foreground": "<%= blue %>",
		"editorUnnecessaryCode.opacity": "#00000040",
		"editorGutter.background": "<%= altBackground %>",
		"editorGutter.modifiedBackground": "<%= modified %>",
		"editorGutter.addedBackground": "<%= added %>",
		"editorGutter.deletedBackground": "<%= deleted %>",
		"editorGutter.commentRangeForeground": "<%= ignored %>",
		
		"// Editor Widget": "//",
		"editorWidget.foreground": "<%= foreground %>",
		"editorWidget.background": "<%= background %>",
		"editorWidget.border": "<%= border %>",
		
		"// Peek View": "//",
		"peekView.border": "<%= border %>",
		"peekViewEditor.background": "<%= background %>",
		
		"// Status Bar": "//",
		"statusBar.background": "<%= background %>",
		"statusBar.foreground": "<%= foreground %>",
		"statusBar.border": "<%= border %>",
		"statusBar.debuggingBackground": "<%= red %>",
		"statusBar.debuggingForeground": "<%= brightBackground %>",
		"statusBar.debuggingBorder": "<%= red %>",
		"statusBar.noFolderBackground": "<%= border %>",
		"statusBarItem.activeBackground": "<%= accentAlpha80 %>",
		"statusBarItem.hoverBackground": "<%= accentAlpha60 %>",
		"statusBarItem.prominentForeground": "<%= brightForeground %>",
		
		"// Title Bar": "//",
		"titleBar.activeBackground": "<%= border %>",
		"titleBar.activeForeground": "<%= altForeground %>",
		"titleBar.inactiveBackground:": "<%= border %>",
		"titleBar.inactiveForeground:": "<%= foreground %>",
		"titleBar.border": "<%= border %>",
		
		"// Breadcrumb": "//",
		"breadcrumb.foreground": "<%= comment %>",
		"breadcrumb.background": "<%= altBackground %>",
		"breadcrumb.focusForeground": "<%= foreground %>",
		"breadcrumb.activeSelectionForeground": "<%= altForeground %>",
		"breadcrumbPicker.background": "<%= background %>",
		
		"// Diff Editor": "//",
		"diffEditor.insertedTextBackground": "<%= addedBackground %>",
		"diffEditor.insertedTextBorder": "<%= altBackground %>",
		"diffEditor.removedTextBackground": "<%= deletedBackground %>",
		"diffEditor.removedTextBorder": "<%= altBackground %>",
		"diffEditor.border": "<%= border %>",
		
		"// Panel": "//",
		"panel.foreground": "<%= foreground %>",
		"panel.background": "<%= background %>",
		"panel.border": "<%= border %>",
		"panelTitle.activeBorder": "<%= accent %>",
		
		"// Git": "//",
		"gitDecoration.addedResourceForeground": "<%= added %>",
		"gitDecoration.modifiedResourceForeground": "<%= modified %>",
		"gitDecoration.deletedResourceForeground": "<%= deleted %>",
		"gitDecoration.untrackedResourceForeground": "<%= untracked %>",
		"gitDecoration.ignoredResourceForeground": "<%= ignored %>",
		"gitDecoration.conflictingResourceForeground": "<%= conflict %>",
		"gitDecoration.submoduleResourceForeground": "<%= submodule %>",
		
		"// Terminal": "//",
		"terminal.background": "<%= background %>",
		"terminal.foreground": "<%= foreground %>",
		"terminal.border": "<%= border %>",
		"terminal.ansiBlack": "<%= background %>",
		"terminal.ansiBrightBlack": "<%= altBackground %>",
		"terminal.ansiWhite": "<%= foreground %>",
		"terminal.ansiBrightWhite": "<%= altForeground %>",
		"terminal.ansiBlue": "<%= blue %>",
		"terminal.ansiBrightBlue": "<%= blue %>",
		"terminal.ansiCyan": "<%= cyan %>",
		"terminal.ansiBrightCyan": "<%= cyan %>",
		"terminal.ansiGreen": "<%= green %>",
		"terminal.ansiBrightGreen": "<%= green %>",
		"terminal.ansiMagenta": "<%= purple %>",
		"terminal.ansiBrightMagenta": "<%= purple %>",
		"terminal.ansiRed": "<%= red %>",
		"terminal.ansiBrightRed": "<%= red %>",
		"terminal.ansiYellow": "<%= yellow %>",
		"terminal.ansiBrightYellow": "<%= yellow %>",
		"terminal.selectionForeground": "<%= foreground %>",
		"terminal.selectionBackground": "<%= accentAlpha40 %>",
		"terminalCursor.background": "<%= accent %>",
		"terminalCursor.foreground": "<%= accent %>",

		"debugToolBar.background": "<%= altBackground %>",
		"debugToolBar.border": "<%= red %>",
		
		"// Settings": "//",
		"settings.headerForeground": "<%= foreground %>",
		"settings.modifiedItemIndicator": "<%= modified %>",
		"settings.dropdownBackground": "<%= altBackground %>",
		"settings.dropdownForeground": "<%= altForeground %>",
		"settings.dropdownBorder": "<%= border %>",
		"settings.dropdownListBorder": "<%= border %>",
		"settings.checkboxBackground": "<%= altBackground %>",
		"settings.checkboxForeground": "<%= accent %>",
		"settings.checkboxBorder": "<%= border %>",
		"settings.textInputBackground": "<%= altBackground %>",
		"settings.textInputForeground": "<%= altForeground %>",
		"settings.textInputBorder": "<%= border %>",
		"settings.numberInputBackground": "<%= altBackground %>",
		"settings.numberInputForeground": "<%= altForeground %>",
		"settings.numberInputBorder": "<%= border %>"

	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "<%= comment %>"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"punctuation.definition.string.begin",
				"punctuation.definition.string.end"
			],
			"settings": {
				"foreground": "<%= yellow %>"
			}
		},
		{
			"name": "Regex",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "<%= orange %>"
			}
		},
		{
			"name": "Template Expression, String Interpolation",
			"scope": [
				"punctuation.definition.template-expression",
				"punctuation.definition.interpolation"
			],
			"settings": {
				"foreground": "<%= orange %>"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable"
			],
			"settings": {
				"foreground": "<%= blue %>"
			}
		},
		{
			"name": "Parameters",
			"scope": [
				"variable.parameter"
			],
			"settings": {
				"foreground": "<%= cyan %>"
			}
		},
		{
			"name": "Constant",
			"scope": [
				"constant"
			],
			"settings": {
				"foreground": "<%= pink %>"
			}
		},
		{
			"name": "Keyword, Modifier",
			"scope": [
				"keyword",
				"storage.modifier"
			],
			"settings": {
				"foreground": "<%= lime %>"
			}
		},
		{
			"name": "Storage Type",
			"scope": [
				"storage.type"
			],
			"settings": {
				"foreground":"<%= cyan %>"
			}
		},
		{
			"name": "Function",
			"scope": [
				"entity.name.function"
			],
			"settings": {
				"foreground": "<%= purple %>"
			}
		},
		{
			"name": "Support Type",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "<%= green %>"
			}
		},
		{
			"name": "Support Property Name",
			"scope": [
				"support.type.property-name"
			],
			"settings": {
				"foreground": "<%= cyan %>"
			}
		},
		{
			"name": "Support Constant",
			"scope": [
				"support.constant"
			],
			"settings": {
				"foreground": "<%= pink %>"
			}
		},
		{
			"name": "Support Function",
			"scope": [
				"support.function"
			],
			"settings": {
				"foreground": "<%= purple %>"
			}
		},
		{
			"name": "Language Variable",
			"scope": [
				"variable.language"
			],
			"settings": {
				"foreground": "<%= lime %>"
			}
		},
		{
			"name": "Class, Interface",
			"scope": [
				"entity.name",
				"support.class"
			],
			"settings": {
				"foreground": "<%= green %>"
			}
		},
		{
			"name": "Decorator, Annotation",
			"scope": [
				"meta.decorator entity.name.function",
				"punctuation.decorator"
			],
			"settings": {
				"foreground": "<%= orange %>"
			}
		},
		{
			"name": "Entity Type",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "<%= orange %>"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"punctuation.definition.tag",
				"entity.name.tag"
			],
			"settings": {
				"foreground": "<%= purple %>"
			}
		},
		{
			"name": "Tag Attribute",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "<%= cyan %>"
			}
		},
		{
			"name": "CSS Class Selectors, CSS Pseudo Selectors, CSS Tag Selectors, CSS Selector Entity",
			"scope": [
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.pseudo-class.css",
				"entity.name.tag.css",
				"punctuation.definition.entity.css"
			],
			"settings": {
				"foreground": "<%= green %>"
			}
		},
		{
			"name": "CSS ID Selectors",
			"scope": [
				"entity.other.attribute-name.id.css"
			],
			"settings": {
				"foreground": "<%= orange %>"
			}
		},
		{
			"name": "CSS Attribute Selectors, CSS Attribute Names",
			"scope":  [
				"meta.attribute-selector",
				"entity.other.attribute-name.css"
			],
			"settings": {
				"foreground" : "<%= cyan %>"
			}
		},
		{
			"name": "Scss Tag Reference ('&')",
			"scope": [
				"entity.name.tag.reference.scss"
			],
			"settings": {
				"foreground": "<%= purple %>"
			}
		}
	]
}